@using DomainModel.Entities;
@model IEnumerable<Song>

@{
    ViewBag.Title = "Index";
}


@foreach (var song in Model)
{
    @song.Name 
    foreach (var item in song.Artists)
    {
        if (item.Group != null)
        {
            @item.Group 
            continue;
        }
        @item.Name  @item.Surname
    }
   <br>
}

